{
  "name": "favorite-folder-tray",
  "version": "1.0.0",
  "description": "Save your favorite folders to open in OS, vscode and terminal.",
  "main": "main.js",
  "repository": "https://github.com/DanilooHenriquee/favorite-folder-tray",
  "scripts": {
    "dev": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist:all": "npm-run-all -p dist:*",
    "dist:linux": "electron-builder --linux",
    "dist:windows": "electron-builder --windows",
    "dist:mac": "electron-builder --mac"
  },
  "keywords": [],
  "author": "DanilooHenriquee <daniloohenriquee@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "electron": "^29.1.6",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1"
  },
  "dependencies": {
    "electron-store": "^8.2.0"
  },
  "build": {
    "appId": "github.com/DanilooHenriquee/favorite-folder-tray",
    "productName": "Favorite folder tray",
    "linux": {
      "category": "Utility",
      "icon": "icon.png",
      "target": [
        "deb",
        "snap",
        "rpm"
      ]
    }
  }
}
